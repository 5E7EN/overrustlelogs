= content body
  div class="table-responsive"
    table id="bs-table" class="table table-hover"
      thead
        tr
           th class="col-lg-8 text-left" data-sort="string" Name

      tbody data-link="row" class="rowlink"
        {{range .Paths}}
          tr
            td class="text-left" data-sort-value="{{.Name}}"
              span class="glyphicon glyphicon-{{.Icon}}" &nbsp;
                a href="{{.Path}}"
                  strong {{.Name}}
        {{end}}