= content body
  div class="container"
    ul class="collection with-header"
      {{if .Breadcrumbs}}
        li class="collection-header"
          a href="/"
            i class="fa fa-home fa-fw"
          {{range .Breadcrumbs}}
            a href={{.Path}}  / {{.Name}}
          {{end}}
      {{end}}
      {{range .Paths}}
        a href="{{.Path}}" class="collection-item"
          i class="fa fa-{{.Icon}} fa-fw"
          span &nbsp; {{.Name}}
      {{end}}
